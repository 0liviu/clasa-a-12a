#include <bits/stdc++.h>

using namespace std;
ifstream f("bac.txt");
int main()
{
    int x,fr[10000],maxim=0;
    while(f>>x)
    fr[x]++;
    for(int i=1;i<=10000;i++)
    {
        if(fr[i]==i)
            k=k+i;
        if(fr[i]==0)
             k=k+0;
        if(fr[i]!=i)
        if(k>maxim)
        {maxim=k;k=0;}
    }
